		{
			"name": "Get Date",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function() {\r",
							"    pm.response.to.have.status(200);\r",
							"})\r",
							"\r",
							"pm.test(\"Header exist\", function(){\r",
							"    pm.response.to.have.header('Access-Control-Expose-Headers');\r",
							"})\r",
							"\r",
							"pm.test(\"Header exist\", function(){\r",
							"    const connectionHeader = pm.response.headers.get(\"Connection\");\r",
							"    pm.expect(connectionHeader).to.eql(\"keep-alive\");\r",
							"    // const headers = pm.response.headers.toObject(); або можна було так\r",
							"    // console.log(headers);\r",
							"})\r",
							"\r",
							"pm.test(\"Response body has expected value\", function() {\r",
							"    const json = pm.response.json(); \r",
							"    // розбирає тіло відповіді як JSON та повертає JavaScript-об'єкт\r",
							"    pm.expect(json.media_type).to.eql(\"image\");\r",
							"})"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
